<h1>Contacts#index</h1>
<p>Find me in app/views/contacts/index.html.erb</p>

<p><%= notice %></p>

<div class="container">
  <div class="col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">
    <h1>問い合わせ一覧</h1>

      <% @contacts.each do |contact| %>
      <p>名前:<%= contact.name %></p>
      <p>メール:<%= contact.email %></p>
      <p>本文:<%= contact.content %></p>
    <% end %>
  </div>
</div>

<%= link_to "問い合わせする", new_contact_path %>
<%= link_to "ブログ一覧画面にもどる", blogs_path %>